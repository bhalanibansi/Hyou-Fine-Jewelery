<div class="v-day-main-section grid-container wrapper">
  {% if section.settings.heading %}
  <h1>{{ section.settings.heading }}</h1>{% endif %}
  {% if section.settings.content %}<p>{{ section.settings.content }}</p>{% endif %}
</div>
{% schema %}
  {
    "name": "valentine day header",
    "settings": [
      {
        "type": "text",
        "id": "heading",
        "label": "Heading"
      },
      {
        "type": "text",
        "id": "content",
        "label": "Content"
      }
    ],
     "presets": [
      {
        "name": "valentine day header"
      }
    ]
  }
{% endschema %}